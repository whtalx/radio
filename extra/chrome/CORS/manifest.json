{
  "name": "CORS",
  "version": "0.1.0",
  "description": "'Access-Control-Allow-Origin: *'",
  "background": {
     "scripts": [ "background.js" ]
  },
  "browser_action": {
     "default_icon": {
        "16": "icons/16.png",
        "19": "icons/19.png",
        "32": "icons/32.png",
        "38": "icons/38.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
     }
  },
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "32": "icons/32.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "permissions": [ "storage", "http://*/*", "<all_urls>", "webRequest", "webRequestBlocking" ],
  "manifest_version": 2
}
