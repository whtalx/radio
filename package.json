{
  "name": "radio",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "author": "whtalx",
  "description": "web radio player",
  "main": "public/electron.js",
  "scripts": {
    "start": "concurrently \"BROWSER=none craco start\" \"wait-on http://localhost:3000 && ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .\"",
    "pack": "electron-builder -w",
    "prepack": "craco build",
    "release": "electron-builder",
    "react": "craco start",
    "electron": "electron .",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.1",
    "electron-is-dev": "^1.1.0",
    "electron-localshortcut": "^3.2.1",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.2",
    "babel-plugin-styled-components": "^1.10.6",
    "concurrently": "^5.0.2",
    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "hls.js": "^0.13.0",
    "prettier": "^1.18.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "redux": "^4.0.4",
    "redux-electron-store": "^0.6.6",
    "styled-components": "^4.4.1",
    "wait-on": "^3.3.0"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.example.radio",
    "productName": "radio",
    "mac": {
      "category": "public.app-category.music"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
